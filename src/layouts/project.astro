---
import MainHead from "../components/MainHead.astro";
import Hero from "../components/Hero.astro";

const pageTitle = "Home"

const navItems = [
	{
		label: "Home",
		path: "/"
	},
	{
		label: "Projects",
		path: "/projects"
	},
	{
		label: "Resume",
		path: "/resume"
	},
	{
		label: "About",
		path: "/about"
	},
	{
		label: "Contact",
		path: "/contact"
	},
];

const skills = [
	{
		label: "React",
		value: "2 Years",
	},
	{
		label: "TypeScript",
		value: "2 Years",
	},
	{
		label: "JQuery",
		value: "4 Years"
	},
	{
		label: "PHP",
		value: "4 Years",
	},
	{
		label: "Graphic Design",
		value: "8 Years",
	},
	{
		label: "Motion Graphics",
		value: "8 Years",
	},
	{
		label: "Video Production",
		value: "8 Years",
	},
];

const sideContent = [
	{
		type: "table",
		heading: "Skills",
		name: "skills",
		content: skills
	}
]

const {content} = Astro.props
---

<html lang="en">
	<head>
		<MainHead	/>
	</head>
	<body>
		{content}
		<!-- <Hero pageTitle={pageTitle} navItems={navItems} sideContent={sideContent} /> -->
	</body>
</html>
